# ===========================================
# DEPLOYMENT CONFIGURATION TEMPLATE
# Copy this file to .env and fill in your values
# ===========================================

# -------------------------------------------
# VERCEL (Frontend Deployment)
# Get token from: https://vercel.com/account/tokens
# -------------------------------------------
VERCEL_TOKEN=
VERCEL_TEAM_ID=
VERCEL_PROJECT_ID=
VERCEL_ORG_ID=

# -------------------------------------------
# RENDER (Backend Deployment)
# Get API key from: https://dashboard.render.com/u/settings
# -------------------------------------------
RENDER_API_KEY=
RENDER_SERVICE_ID=

# -------------------------------------------
# SUPABASE (Database)
# Get from: https://supabase.com/dashboard/project/_/settings/api
# -------------------------------------------
SUPABASE_ACCESS_TOKEN=
SUPABASE_PROJECT_REF=
SUPABASE_URL=
SUPABASE_KEY=

# -------------------------------------------
# MARKETO INTEGRATION
# Get from: Marketo Admin > Integration > LaunchPoint > New Service
# -------------------------------------------
MARKETO_CLIENT_ID=
MARKETO_CLIENT_SECRET=
MARKETO_BASE_URL=https://xxx-xxx-xxx.mktorest.com
MARKETO_IDENTITY_URL=https://xxx-xxx-xxx.mktorest.com/identity
MARKETO_WEBHOOK_SECRET=your-random-secret-string
MARKETO_EMAIL_CAMPAIGN_ID=

# -------------------------------------------
# APP CONFIGURATION
# -------------------------------------------
DEBUG=false
LOG_LEVEL=INFO
